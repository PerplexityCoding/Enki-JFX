package fr.nfan.ui.deckProperties

import javafx.scene.layout.*
import javafx.scene.control.*

component AnkiDeckPropertiesLatexTab {
	Tab {
		text : "Latex",
		closable : false,
		content : [
			VBox id latex {
				style : "-fx-padding: 5 5 5 5",
				children : [
					Label {
						text : "Header"
					},
					TextArea id header,
					Label {
						text : "Footer"
					},
					TextArea id footer
				]
			}
		]
	}
}